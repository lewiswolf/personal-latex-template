{
	"editor.insertSpaces": false,
	"editor.detectIndentation": false,
	"latex-workshop.bibtex-format.tab": "tab",
	"latex-workshop.latex.clean.fileTypes": [
		"*.aux",
		"*/*.aux",
		"*.bbl",
		"*.blg",
		"*.fdb_latexmk",
		"*.fls",
		"*.log",
		"*.out",
		"*.synctex(busy)",
		"*.synctex.gz",
		"*.synctex.gz(busy)",
		"*.toc"
	],
	"latex-workshop.latex.tools": [
		{
			"name": "latexmk",
			"command": "pipenv",
			"args": [
				"run",
				"latexmk",
				"--shell-escape",
				"-synctex=1",
				"-interaction=nonstopmode",
				"-file-line-error",
				"-pdf",
				"-outdir=%OUTDIR%",
				"%DOC%"
			],
			"env": {}
		},
		{
			"name": "lualatexmk",
			"command": "pipenv",
			"args": [
				"run",
				"latexmk",
				"-synctex=1",
				"-interaction=nonstopmode",
				"-file-line-error",
				"-lualatex",
				"-outdir=%OUTDIR%",
				"%DOC%"
			],
			"env": {}
		},
		{
			"name": "latexmk_rconly",
			"command": "pipenv",
			"args": [
				"run",
				"latexmk",
				"%DOC%"
			],
			"env": {}
		},
		{
			"name": "pdflatex",
			"command": "pipenv",
			"args": [
				"run",
				"pdflatex",
				"--shell-escape",
				"-synctex=1",
				"-interaction=nonstopmode",
				"-file-line-error",
				"%DOC%"
			],
			"env": {}
		},
		{
			"name": "bibtex",
			"command": "pipenv",
			"args": [
				"run",
				"bibtex",
				"%DOCFILE%"
			],
			"env": {}
		},
		{
			"name": "rnw2tex",
			"command": "pipenv",
			"args": [
				"run",
				"Rscript",
				"-e",
				"knitr::opts_knit$set(concordance = TRUE); knitr::knit('%DOCFILE_EXT%')"
			],
			"env": {}
		},
		{
			"name": "jnw2tex",
			"command": "pipenv",
			"args": [
				"run",
				"julia",
				"-e",
				"using Weave; weave(\"%DOC_EXT%\", doctype=\"tex\")"
			],
			"env": {}
		},
		{
			"name": "jnw2texmintex",
			"command": "pipenv",
			"args": [
				"run",
				"julia",
				"-e",
				"using Weave; weave(\"%DOC_EXT%\", doctype=\"texminted\")"
			],
			"env": {}
		},
		{
			"name": "tectonic",
			"command": "pipenv",
			"args": [
				"run",
				"tectonic",
				"--synctex",
				"--keep-logs",
				"%DOC%.tex"
			],
			"env": {}
		}
	],
	"latex-workshop.latex.autoBuild.run": "onSave",
	"latex-workshop.latex.autoClean.run": "onBuilt",
	"latex-workshop.view.pdf.viewer": "tab",
	"latex-workshop.chktex.enabled": false,
	"cSpell.language": "en-GB",
	"cSpell.enabledLanguageIds": [
		"latex"
	]
}